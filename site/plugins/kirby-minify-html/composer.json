{
    "name": "afbora/kirby-minify-html",
    "description": "Enable minify HTML output for Kirby 3",
    "keywords": [
        "kirby",
        "kirby-3",
        "kirby-cms",
        "kirby3-plugin",
        "kirby-plugin"
    ],
    "version": "1.2.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet Bora",
            "email": "ahmet.bora@pixelturk.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "getkirby/composer-installer": "^1.1.4",
        "voku/html-min": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
